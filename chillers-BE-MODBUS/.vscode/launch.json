  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}"
    },    
    {
      "type": "node",
      "request": "launch",
      "name": "Launch node server 3100",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\index.js",
      "outputCapture": "std"
    }
  ]
}
